{
  "compilerOptions": {
    "downlevelIteration": true,
    "lib": ["dom", "es2020"],
    "noEmit": true,
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": true,
    "declaration": true,
    "isolatedModules": true,
    "types": ["node", "jest"],
    "typeRoots": ["./types", "./node_modules/@types"],
    "strictNullChecks": true
  }
}
